
@{
    ViewBag.Title = "Received";
}

<div class="jumbotron">
    <div class="container">
        <div class="row">
            <h2>Received Texts</h2>
            @foreach (var x in ViewBag.messagebuffer)
            {
                <div style="color:orangered">
                    @x<br>
                </div>
            }
        </div>
    </div>
</div>

